                      </b-tabs>\n\n\n\n                                    <!--<ul class=\"nav nav-tabs border-tab nav-primary\" id=\"info-tab\" role=\"tablist\">-->\n                                        <!--<li class=\"nav-item\"><a class=\"nav-link active\" id=\"info-home-tab\" data-toggle=\"tab\" href=\"#info-home\" role=\"tab\" aria-selected=\"true\">CALL</a>-->\n                                            <!--<div class=\"material-border\"></div>-->\n                                        <!--</li>-->\n                                        <!--<li class=\"nav-item\"><a class=\"nav-link\" id=\"profile-info-tab\" data-toggle=\"tab\" href=\"#info-profile\" role=\"tab\" aria-selected=\"false\">STATUS</a>-->\n                                            <!--<div class=\"material-border\"></div>-->\n                                        <!--</li>-->\n                                        <!--<li class=\"nav-item\"><a class=\"nav-link\" id=\"contact-info-tab\" data-toggle=\"tab\" href=\"#info-contact\" role=\"tab\" aria-selected=\"false\">PROFILE</a>-->\n                                            <!--<div class=\"material-border\"></div>-->\n                                        <!--</li>-->\n                                    <!--</ul>-->\n                                      <!--  <div class=\"tab-content\" id=\"info-tabContent\">\n                                            <div class=\"tab-pane fade show active\" id=\"info-home\" role=\"tabpanel\" aria-labelledby=\"info-home-tab\">\n                                                <div class=\"people-list\">\n                                                    <ul class=\"list digits\">\n                                                        <li class=\"clearfix\" v-for=\"(user,index) in users\" :key=\"index\" @click=\"setActiveuser(user.id)\">\n                                                            <img class=\"rounded-circle user-image\" :src='getImgUrl(user.thumb)' alt=\"\">\n                