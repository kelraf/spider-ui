ass=\"chart-overflow\" id=\"bar-chart1\"></div>\n                    <GChart\n                      class=\"chart-overflow\" \n                      id=\"bar-chart1\"\n                      type=\"ColumnChart\"\n                      :data=\"bar_chart.chartData_1\"\n                      :options=\"bar_chart.options_1\"\n                    />\n                  </div>\n                </div>\n              </div>\n              <div class=\"col-sm-12 col-xl-6\">\n                <div class=\"card\">\n                  <div class=\"card-header\">\n                    <h5>Bars Chart <span class=\"digits\">2</span></h5>\n                  </div>\n                  <div class=\"card-body chart-block\">\n                    <GChart\n                      class=\"chart-overflow\" \n                      id=\"bar-chart2\"\n                      type=\"BarChart\"\n                      :data=\"bar_chart.chartData_1\"\n                      :options=\"bar_chart.options_1\"\n                    />\n                  </div>\n                </div>\n              </div>\n              <div class=\"col-sm-12\">\n                <div class=\"card\">\n                  <div class=\"card-header\">\n                    <h5>Line Chart</h5>\n                  </div>\n                  <div class=\"card-body chart-block\">\n                    <GChart\n                      class=\"chart-overflow\" \n                      id=\"line-chart\"\n                      type=\"LineChart\"\n                      :data=\"line_chart.chartData_1\"\n                      :options=\"line_chart.options_1\"\n                    />\n                  </div>\n                </div>\n              </div>\n              <div class=\"col-sm-12\">\n                <div class=\"card\">\n                  <div class=\"card-header\">\n                    <h5>Combo Chart</h5>\n                  </div>\n                  <div class=\"card-body chart-block\">\n                    <GChart\n                      c