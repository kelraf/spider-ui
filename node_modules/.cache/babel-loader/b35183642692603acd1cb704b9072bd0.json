              <div class=\"form-group\">\n                            <datepicker input-class=\"datepicker-here form-control digits\" :format=\"format\" v-model=\"newevent.startDate\" ></datepicker>\n                          </div>\n                        </div>\n                        <div class=\"col-12\">\n                          <label class=\"f-w-600\">End Date:</label>\n                          <div class=\"form-group\">\n                            <datepicker input-class=\"datepicker-here form-control digits\" :format=\"format\" v-model=\"newevent.endDate\" ></datepicker>\n                          </div>\n                        </div>\n                        <div class=\"col-12\">\n                          <button class=\"btn btn-primary\">Add Event</button>\n                        </div>\n                      </div>\n                    </form>\n                  </div>\n                </div>\n              </div>\n              <div class=\"col-xl-9 col-md-8\">\n                <div class=\"card\">\n                    <div class=\"card-header\">\n                      <h5 class=\"card-title\">Basic</h5>\n                      <span>This is the basic example having navigation button to navigate next and previous months and today button. This basic example lists all the events on the calendar.</span>\n                    </div>\n                    <div class=\"card-body\">\n                      <div class=\"row\">\n                        <div class=\"col-md-12\">\n\n                          <calendar-view\n                            :show-date=\"showDate\"\n                            :events=\"events\"\n                            :enable-drag-drop=\"true\"\n                            @drop-on-date=\"onDrop\"\n                            class=\"theme-default holiday-us-traditional holiday-us-official\">\n\n                            <calendar-view-header slot=\"header\" slot-scope=\"t\" :header-props=\"t.headerProps\" @input=\"setShowDate\" />\n                          </calendar-view>\n\n                        </div>\n                      </div>\n                    </div>\n                </div>\n              </div>               \n            </div>\n        </div>\n        <!-- Container-fluid Ends-->\n    </div>\n</template>\n\n<script>\n\nimport { CalendarView, CalendarViewHeader, CalendarMathMixin} from \"vue-simple-calendar\";\n\nimport Datepicker from 'vuejs-datepicker';\n\nexport default {\n  data() {\n    return {\n      format: \"yyyy-MM-dd\",\n\n      newevent:{\n        title:'',\n        startDate:'',\n        endDate:''\n      },\n      showDate: new Date(),\n      events: [\n        {\n          startDate: '2019-4-17',\n          endDate: '2019-4-19',\n          title: 'Sample event 1',\n        },\n        {\n          startDate: '2019-4-21',\n          endDate: '2019-4-21',\n          title: 'Sample event 2',\n        },\n        {\n          startDate: '2019-4-23',\n          endDate: '2019-4-23',\n          title: 'Sample event 3',\n        },\n        {\n          startDate: '2019-4-25',\n          endDate: '2019-4-25',\n          title: 'Sample event 4',\n        },\n        {\n          startDate: '2019-4-29',\n          endDate: '2019-4-29',\n          title: 'Sample event 5',\n        },\n      ]\n    }\n  },\n  components: {\n    CalendarView,\n    CalendarViewHeader,\n    Datepicker\n  },\n  mixins: [CalendarMathMixin],\n  methods: {\n    setShowDate(d) {\n      this.showDate = d;\n    },\n    onDrop(event, date) {\n      this.message = `You dropped ${event.id} on ${date.toLocaleDateString()}`\n      // Determine the delta between the old start date and the date chosen,\n      // and apply that delta to both the start and end date to move the event.\n      const eLength = this.dayDiff(event.startDate, date)\n      event.originalEvent.startDate = this.addDays(event.startDate, eLength)\n      event.originalEvent.endDate = this.addDays(event.endDate, eLength)\n    },\n    addEvent(){\n      this.events.push({\n          title:this.newevent.title,\n          startDate:this.newevent.startDate,\n          endDate:this.newevent.endDate,\n      });\n      this.newevent.title = \"\",\n      this.newevent.startDate = \"\",\n      this.newevent.endDate = \"\"\n    }\n  }\n}\n</script>"],"sourceRoot":"src/pages/calendar"}]}